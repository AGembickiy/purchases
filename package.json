{
  "name": "purchases",
  "version": "1.0.0",
  "description": "Система управления закупками с Django backend и Vue.js frontend",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run css:watch\" \"npm run vue:dev\"",
    "build": "npm run css:build && npm run vue:build",
    "css:watch": "tailwindcss -i ./static/src/input.css -o ./static/css/output.css --watch",
    "css:build": "tailwindcss -i ./static/src/input.css -o ./static/css/output.css --minify",
    "vue:dev": "vite",
    "vue:build": "vue-tsc && vite build",
    "vue:preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AGembickiy/purchases.git"
  },
  "keywords": [
    "purchases",
    "django",
    "vue",
    "typescript",
    "tailwind"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AGembickiy/purchases/issues"
  },
  "homepage": "https://github.com/AGembickiy/purchases#readme",
  "dependencies": {
    "@vueuse/core": "^11.2.0",
    "axios": "^1.7.9",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.4.31",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
